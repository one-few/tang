<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.ttcxy.tangtang.mapper.UserMapper">

    <select id="selectUserByName" resultType="net.ttcxy.tangtang.entity.User">
        select * from entity_user where username = #{username}
    </select>

   <insert id="insertUser" parameterType="net.ttcxy.tangtang.entity.User">
       insert entity_user (id,username,password) values (#{id},#{username},#{password})
   </insert>
</mapper>